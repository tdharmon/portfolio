{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [false],
    "jsx-no-multiline-js": [false],
    "jsx-boolean-value": ["never"]
  },
  "rulesDirectory": [],
  "exclude": ["node_modules"]
}
